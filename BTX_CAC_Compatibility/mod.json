{
	"Name": "BTX_CAC_Compatibility",
	"Enabled": true,

	"Description": "Modifies BTX (and BTXMinusWeapons) for CAC.",
	"Author": "mcb",
	"Website": "",
	"Version": "0.1.2.0",

	"DependsOn": [ "BT_Extended", "BT_Extended_Timeline", "BT_Extended_CE", "BT_Extended_3050", "BT_Extended_Clans", "Custom Ammo Categories" ],
	"OptionallyDependsOn": [ "BTXMinusWeapons" ],
	
	"DLL": "BTX_CAC_CompatibilityDll.dll",
	
	"Settings": {
		"Use4LimitOnContractIds": [],
		"ReplaceInItemCollections": {
			"Weapon_Inferno_Inferno2_0-STOCK": "Ammo_AmmunitionBox_Generic_SRM_Inferno",
			"Weapon_Inferno_Inferno2_1-Harvester": "Ammo_AmmunitionBox_Generic_SRM_Inferno",
			"Weapon_Inferno_Inferno2_2-Marklin_Mini": "Ammo_AmmunitionBox_Generic_SRM_Inferno"
		}
	},
	
	"Manifest": [
		{
		  "Type": "WeaponDef",
		  "Path": "weapons",
		  "ShouldMergeJSON": true
		},
		{
		  "Type": "WeaponDef",
		  "Path": "weaponoverride"
		},
		{
		  "Type": "AmmunitionDef",
		  "Path": "ammo"
		},
		{
		  "Type": "AmmunitionBoxDef",
		  "Path": "ammobox"
		},
		{
		  "Type": "ItemCollectionDef",
		  "Path": "itemCollectionAppend",
		  "ShouldAppendText": true
		},
		{
            "Type": "ShipModuleUpgrade",
            "Path": "shipUpgrades"
        }
	]
}
